

@font-face {
  font-family: 'Enriqueta';
  src: url('<%= asset_path('Enriqueta.ttf') %>');
  font-weight: normal;
  font-style: normal;
}




@font-face {
    font-family: 'CapsuulaRegular';
    src: url('<%= asset_path('Capsuula-webfont.eot') %>');
    src: url('<%= asset_path('Capsuula-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('Capsuula-webfont.woff') %>') format('woff'),
         url('<%= asset_path('Capsuula-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('Capsuula-webfont.svg#CapsuulaRegular') %>') format('svg');
    font-weight: normal;
    font-style: normal;

}



@font-face {
  font-family: 'fontello';
  src: url('<%= asset_path('fontello.eot?69207018') %>');
